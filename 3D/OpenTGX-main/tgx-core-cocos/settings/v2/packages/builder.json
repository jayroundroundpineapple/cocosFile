{
  "__version__": "1.3.2",
  "bundleConfig": {
    "custom": {
      "default": {
        "displayName": "i18n:builder.asset_bundle.defaultConfig",
        "configs": {
          "native": {
            "preferredOptions": {
              "isRemote": false,
              "compressionType": "merge_dep"
            }
          },
          "web": {
            "preferredOptions": {
              "isRemote": false,
              "compressionType": "merge_all_json"
            },
            "fallbackOptions": {
              "compressionType": "merge_dep"
            }
          },
          "miniGame": {
            "fallbackOptions": {
              "isRemote": false,
              "compressionType": "merge_dep"
            },
            "configMode": "overwrite",
            "overwriteSettings": {
              "wechatgame": {
                "compressionType": "subpackage"
              }
            }
          }
        }
      }
    }
  }
}
